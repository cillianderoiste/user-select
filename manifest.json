{

  "manifest_version": 2,
  "name": "user-select",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/icon.ico"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["user_select.js"]
    }
  ]

}
